<p id="notice"><%= notice %></p>

<h1>Picture Uploads</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @picture_uploads.each do |picture_upload| %>
      <tr>
        <td><%= picture_upload.title %></td>
        <td><%= picture_upload.description %></td>
        <td><%= link_to 'Show', picture_upload %></td>
        <td><%= link_to 'Edit', edit_picture_upload_path(picture_upload) %></td>
        <td><%= link_to 'Destroy', picture_upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Picture Upload', new_picture_upload_path %>
